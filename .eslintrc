{
   "parser": "@babel/eslint-parser",
   "extends": [
      "airbnb-base"
   ],
   "overrides": [
   ],
   "parserOptions": {
       "ecmaVersion": "latest",
       "sourceType": "module",
       "requireConfigFile": false
   },
   "rules": {
      "no-console": "off",
      "no-shadow": "off",
      "operator-linebreak": [
         "error",
         "after",
         {
           "overrides": {
             ":": "before"
           }
         }
       ]
   },
   "env": {
      "browser": true,
      "node": true,
      "es2021": true
   }
}
